<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhta.mybatis.DisableDateMapper">
	<select id="getDisable" parameterType="string" resultType="com.jhta.project.vo.DisableDateVo">
		select * from disabledate where ps_email=#{value}
	</select>
	<update id="updateDis" parameterType="com.jhta.project.vo.DisableDateVo">
		update disabledate set disday=#{disday},disDate=#{disDate} where ps_email=#{ps_email}
	</update>
</mapper>