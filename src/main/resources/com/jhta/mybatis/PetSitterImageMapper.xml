<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhta.mybatis.PetSitterImageMapper">
    <insert id="insert" parameterType="hashmap">
    	insert into petsitterimage values(SEQ_PETSITTERIMAGE_PIMG_NUM.nextval,#{ps_email},#{pimg_savefilename},#{pimg_orgfilename}) 
    </insert>
    <select id="getImg" parameterType="string" resultType="petsitterimage">
		select * from petsitterimage where ps_email=#{value}
	</select>
	<delete id="delete" parameterType="int">
		delete from petsitterimage where pimg_num=#{pimg_num}
	</delete>
</mapper>
